{
    "compilerOptions": {
        "target": "esnext",
        "module": "nodenext",
        "allowImportingTsExtensions": true,
        "rewriteRelativeImportExtensions": true,
        "verbatimModuleSyntax": true,
        "strict": true,
        "skipLibCheck": true,
        "noUncheckedIndexedAccess": true,
        "noImplicitAny": true
    }
}
